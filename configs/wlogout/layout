{
    "label" : "reboot",
    "action" : "killall brave; rm ~/.config/hypr/removeWindowLocksSettings; systemctl reboot",
    "text" : "Reboot",
    "keybind" : "r"
}
{
    "label" : "shutdown",
    "action" : "killall brave;rm ~/.config/hypr/removeWindowLocksSettings;systemctl poweroff",
    "text" : "Shutdown",
    "keybind" : "s"
}
{
    "label" : "logout",
    "action" : "killall brave;rm ~/.config/hypr/removeWindowLocksSettings;hyprctl dispatch exit 0",
    "text" : "Logout",
    "keybind" : "l"
}
{
    "label" : "suspend",
    "action" : "systemctl suspend",
    "text" : "Suspend",
    "keybind" : "u"
}
